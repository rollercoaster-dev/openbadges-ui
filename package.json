    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.0.0",
    "primeicons": "^6.0.0",
    "primevue": "^3.0.0",
    "terser": "^5.39.0",
    "typescript": "^4.0.0",
    "vite": "^4.0.0",
    "vue": "^3.0.0"
